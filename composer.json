{
    "name": "tcytra/enraiged-tables",
    "description": "Enraiged Tables provides table handling features for Laravel Enraiged projects.",
    "keywords": ["tables", "enraiged"],
    "homepage": "https://github.com/tcytra/enraiged-core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Todd Cytra",
            "email": "tcytra@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^12.0",
        "phpoffice/phpspreadsheet": "^1.30",
        "tcytra/enraiged-exports": "^0.4",
        "tcytra/enraiged-support": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "Enraiged\\Tables\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=enraiged-tables --ansi --force"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enraiged\\Tables\\TablesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
